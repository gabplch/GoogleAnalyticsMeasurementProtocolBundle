{
    "name": "gablch/google-analytics-measurement-protocol-bundle",
    "description": "Provides communication with Google Analytics, using measurement protocol in Symfony applications.",
    "keywords": ["bundle", "symfony", "google analytics", "measurement protocol", "backend tracking", "backend events"],
    "homepage": "https://github.com/gabplch/GoogleAnalyticsMeasurementProtocolBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yarysh Vlad",
            "email": "yarysh.vlad@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/gabplch/GoogleAnalyticsMeasurementProtocolBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "yarysh.vlad@gmail.com",
        "issues": "https://github.com/gabplch/GoogleAnalyticsMeasurementProtocolBundle/issues"
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^v6.2",
        "symfony/http-client": "^v6.2",
        "symfony/runtime": "^v6.2",
        "symfony/serializer": "^v6.2",
        "symfony/validator": "^6.2",
        "symfony/yaml": "^v6.2"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "escapestudios/symfony2-coding-standard": "^3.13",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "slam/phpstan-extensions": "^6.0",
        "symfony/phpunit-bridge": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Gabplch\\GoogleAnalyticsMeasurementProtocolBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gabplch\\GoogleAnalyticsMeasurementProtocolBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
