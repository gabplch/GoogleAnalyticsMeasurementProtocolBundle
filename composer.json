{
    "name": "gablch/google-analytics-measurement-protocol-bundle",
    "description": "Provides communication with Google Analytics with measurement protocol in Symfony applications.",
    "keywords": ["bundle", "symfony", "google analytics", "measurement protocol", "backend tracking", "backend events"],
    "homepage": "https://github.com/gabplch/GoogleAnalyticsMeasurementProtocolBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Yarysh Vlad",
            "email": "yarysh.vlad@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/gabplch/GoogleAnalyticsMeasurementProtocolBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "yarysh.vlad@gmail.com",
        "issues": "https://github.com/gabplch/GoogleAnalyticsMeasurementProtocolBundle/issues"
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "6.2.*",
        "symfony/http-client": "6.2.*",
        "symfony/runtime": "6.2.*",
        "symfony/serializer": "6.2.*",
        "symfony/yaml": "6.2.*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.2.*",
        "symfony/css-selector": "6.2.*",
        "symfony/phpunit-bridge": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Gabplch\\GoogleAnalyticsMeasurementProtocolBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Gabplch\\GoogleAnalyticsMeasurementProtocolBundle\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
